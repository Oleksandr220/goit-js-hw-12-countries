{{#each this}}
<div class="conteiner">
        <h2 class="name-country"> {{name}} </h2>
<div class="card">
    <div class="card-body">
    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    <p>Langueges:</p>
    <ul class="languages-country">
    {{#each languages}}
        <li class="list-lang"> {{name}} </li>
    {{/each}}
    </ul>
    </div>
    <div class="card-img">
<img src="{{flag}}" alt="{{name}}" height="380px" width="600px">
    </div>
</div>
</div>

{{/each}}